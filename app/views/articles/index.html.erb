<div class="main">

	<table>
		<th>Title</th>
		<th>Body</th>
		<th>Rating</th>

		<% @articles.each do |a| %>
			<tr>
				<td>a.title</td>
				<td>a.body</td>
				<td>a.rating</td>
			</tr>
			<tr>
				<td>The Shiney App</td>
				<td>by Sean R.</td>
				<td>5 Stars</td>
			</tr>

		<% end %>
	</table>

</div>
